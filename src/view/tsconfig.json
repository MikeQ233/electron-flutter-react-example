{
  "extends": "../../tsconfig.settings.json",
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "sourceMap": true,
    "resolveJsonModule": true,
    "jsx": "react-jsx",
    "paths": {
      "@/*": ["src/*"],
      "@view/*": ["src/view/*"],
      "@flutter/*": ["src/flutter/*"]
    }
  }
}
